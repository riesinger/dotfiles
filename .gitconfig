[alias]
	s = status
	a = add --all
	cm = commit -S -m
	cs = commit -S
	pushhead = push -u origin HEAD
	tree = log --oneline --decorate --all --graph
	c = commit
[user]
	name = riesinger
	email = mail@pascal-riesinger.de
[gpg]
	program = /usr/bin/gpg
[pull]
	ff = only
[commit]
	gpgsign = true
[init]
	branch = main